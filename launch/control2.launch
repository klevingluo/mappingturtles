<?xml version="1.0"?>
<launch>
  <!-- turtlebot_teleop_key already has its own built in velocity smoother -->
  <node pkg="actionlib" name="axclient2" type="axclient.py" output="screen" args="/robot_1/exploration" />
  <node pkg="turtlebot_teleop" name="turtlebot_teleop_keyboard" type="turtlebot_teleop_key"   output="screen">
    <param name="scale_linear" value="0.5" type="double"/>
    <param name="scale_angular" value="1.5" type="double"/>
    <remap from="turtlebot_teleop_keyboard/cmd_vel" to="/robot_1/cmd_vel"/>
  </node>
</launch>
